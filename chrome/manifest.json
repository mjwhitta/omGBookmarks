{
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "description": "Reads in bookmarks from https://www.google.com/bookmarks",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "oh-my-GoogleBookmarks",
  "omnibox": {"keyword": "omg"},
  "permissions": [
    "contextMenus",
    "https://www.google.com/bookmarks",
    "notifications",
    "storage",
    "tabs"
  ],
  "version": "1.0.4"
}
